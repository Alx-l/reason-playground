bs_package_flags = -bs-package-map reason-playground
src_root_dir = /Users/AlexL/Projects/scratchpad-reason
bsc = /Users/AlexL/Projects/scratchpad-reason/node_modules/bs-platform/lib/bsc.exe
bsdep = /Users/AlexL/Projects/scratchpad-reason/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-suffix -nostdlib -I '/Users/AlexL/Projects/scratchpad-reason/node_modules/bs-platform/lib/ocaml' -bs-super-errors -color always
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
namespace = -ns ReasonPlayground
bsb_dir_group = 0
refmt = /Users/AlexL/Projects/scratchpad-reason/node_modules/bs-platform/lib/refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
bsc_lib_includes = -I . -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/index.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/index.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/index.mlast.d : build_deps src/index.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/index-ReasonPlayground.cmj |  $src_root_dir/src/index.bs.js src/index-ReasonPlayground.cmi : build_cmj_cmi src/index.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output es6:src
build src/utils.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/utils.re
build src/utils.mlast.d : build_deps src/utils.mlast
build src/utils-ReasonPlayground.cmj |  $src_root_dir/src/utils.bs.js src/utils-ReasonPlayground.cmi : build_cmj_cmi src/utils.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output es6:src
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -c ${in}
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build ReasonPlayground.cmi : build_package ReasonPlayground.mlmap
build build.ninja : phony  ||  ReasonPlayground.cmi src/utils.mlast.d src/index.mlast.d
